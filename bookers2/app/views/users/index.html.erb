<%= render "books/list", book: @book, user: @user %>

<h2>Users</h2>

<table class=table>
  <tr>
    <th>image</th>
    <th>name</th>
    <th colspan="3"></th>
  </tr>
   <% @users.each do |user| %>
   <tr>
     <th><%= attachment_image_tag @user, :image, fallback: "no_image.png", size: "60x60" %></th>
     <th><%= user.name %></th>
     <th><%= link_to "Show", user_path(user.id) %></th>
  </tr>
  <% end %>
</table>
